---
import { getRelativeLocaleUrl } from 'astro:i18n';

// Redirect to the default locale (en)
const defaultLocaleUrl = getRelativeLocaleUrl('en', '');
---

<!-- <meta http-equiv="refresh" content={`0;url=${defaultLocaleUrl}`} /> -->
<script is:inline>
    window.location.replace(`${import.meta.env.BASE_URL}${'en'}/`);
</script>